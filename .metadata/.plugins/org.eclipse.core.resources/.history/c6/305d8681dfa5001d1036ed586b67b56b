package collections;

import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;

public class SortHashMap {
	
	public static void main(String[] args) {
		
	  Map<Integer,String> sortvalues=new HashMap<>();
	  
	  sortvalues.put(1,"Hello");
	  sortvalues.put(8,"Hey");
	  sortvalues.put(0,"Hyy");
	  sortvalues.put(6,"heya");
	  sortvalues.put(5,"Byee");
	  
	  System.out.println("The Elements In HashMap Are: "+ sortvalues);
	  
	  for(Map.Entry<Integer,String> e:sortvalues.entrySet());
	  {
	     //System.out.println(e);
	   	
	}

   }
}

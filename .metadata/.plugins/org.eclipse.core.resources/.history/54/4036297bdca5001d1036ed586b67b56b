package collections;

import java.util.HashMap;
import java.util.Map;

public class SortHashMap {
	
	public static void main(String[] args) {
		
	  Map<Integer,String> sortvalues=new HashMap<>();
	  
	  sortvalues.put(1,"Hello");
	  sortvalues.put(2,"Hey");
	  sortvalues.put(3,"Hyy");
	  sortvalues.put(4,"heya");
	  sortvalues.put(5,"Byee");
	  
	  System.out.println("The Elements In HashMap Are: "+ sortvalues);
		
	}

}

package collections;

import java.util.ArrayList;
import java.util.Collections;


public class ComparabletorArrayList {
	
	
	 public class Laptop 
	 {
		 private String Name;
		 private String Brand;
		 private int Price;
		 
		  
		public Laptop(String name, String brand, int price) {
			super();
			Name = name;
			Brand = brand;
			Price = price;
		}
		
		public Laptop(String name2, String brand2, String string) {
			// TODO Auto-generated constructor stub
		}

		public String getName() {
			return Name;
		}
		public void setName(String name) {
			Name = name;
		}
		public String getBrand() {
			return Brand;
		}
		public void setBrand(String brand) {
			Brand = brand;
		}
		public int getPrice() {
			return Price;
		}
		public void setPrice(int price) {
			Price = price;
		}
		@Override
		public String toString() {
			return "Laptop [Name=" + Name + ", Brand=" + Brand + ", Price=" + Price + "]";
		}
		 
	 }

	public static void main(String[] args) {
		
		ArrayList<Laptop> sorts= new ArrayList<>();
		
		sorts.add(new Laptop("PC","Dell","200"));
		sorts.add(new Laptop("Refrigerator","Whirpool","900"));
		sorts.add(new Laptop("Television","Samsung","1000"));
		sorts.add(new Laptop("MobilePhone","Vivo","800"));
		
		System.out.println("The ArrayList are: " +sorts);
		
		//Collections.sort();
	}
	
} 
